from tkinter import *
from tkinter import messagebox
from selenium import webdriver
def guitt():
    hoten = username.get()
    dates = date.get()
    cancuoc = cmnd.get()
    sodienthoai = sdt.get()
    diachi = dc.get()
    phuongtien = pt.get()
    bienkiemsoat = bks.get()
    noiden = nd.get()
    noiohientai = noht.get()
    trong21ngay = haimot.get()
    bieuhien = bh.get()
    driver = webdriver.Chrome()
    driver.get('https://docs.google.com/forms/d/e/1FAIpQLSePsHfLFUuTvhvhkuA7MisAdEER4V59v1tp56ytYdtzwNW0-g/viewform')
    driver.find_element_by_xpath('/html/body/div[1]/div[2]/form/div[2]/div/div[2]/div[1]/div/div/div[2]/div/div[1]/div/div[1]/input').send_keys(hoten)
    driver.find_element_by_xpath('/html/body/div[1]/div[2]/form/div[2]/div/div[2]/div[2]/div/div/div[2]/div/div[1]/div/div[1]/input').send_keys(dates)
    driver.find_element_by_xpath('/html/body/div[1]/div[2]/form/div[2]/div/div[2]/div[3]/div/div/div[2]/div/div[1]/div/div[1]/input').send_keys(cancuoc)
    driver.find_element_by_xpath('/html/body/div[1]/div[2]/form/div[2]/div/div[2]/div[4]/div/div/div[2]/div/div[1]/div/div[1]/input').send_keys(sodienthoai)
    driver.find_element_by_xpath('/html/body/div[1]/div[2]/form/div[2]/div/div[2]/div[5]/div/div/div[2]/div/div[1]/div/div[1]/input').send_keys(diachi)
    driver.find_element_by_xpath('/html/body/div[1]/div[2]/form/div[2]/div/div[2]/div[6]/div/div/div[2]/div/div[1]/div/div[1]/input').send_keys(phuongtien)
    driver.find_element_by_xpath('/html/body/div[1]/div[2]/form/div[2]/div/div[2]/div[7]/div/div/div[2]/div/div[1]/div/div[1]/input').send_keys(bienkiemsoat)
    driver.find_element_by_xpath('/html/body/div[1]/div[2]/form/div[2]/div/div[2]/div[8]/div/div/div[2]/div/div[1]/div/div[1]/input').send_keys(noiden)
    driver.find_element_by_xpath('/html/body/div[1]/div[2]/form/div[2]/div/div[2]/div[9]/div/div/div[2]/div/div[1]/div/div[1]/input').send_keys(noiohientai)
    driver.find_element_by_xpath('/html/body/div[1]/div[2]/form/div[2]/div/div[2]/div[10]/div/div/div[2]/div/div[1]/div/div[1]/input').send_keys(trong21ngay)
    driver.find_element_by_xpath('/html/body/div[1]/div[2]/form/div[2]/div/div[2]/div[11]/div/div/div[2]/div/div[1]/div/div[1]/input').send_keys(bieuhien)
    driver.find_element_by_xpath('/html/body/div[1]/div[2]/form/div[2]/div/div[3]/div[1]/div[1]/div/span/span').click()
def thongke():
